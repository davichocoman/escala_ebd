{
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/admin.html",
      "destination": "/templates/admin.html"
    },
    {
      "source": "/login",
      "destination": "/templates/login.html"
    },
    {
      "source": "/login.html",
      "destination": "/templates/login.html"
    },
    {
      "source": "/portal",
      "destination": "/templates/portal.html"
    },
    {
      "source": "/portal.html",
      "destination": "/templates/portal.html"
    },
    {
      "source": "/patrimonio",
      "destination": "/templates/painel.html"
    },
    {
      "source": "/(.*)",
      "destination": "/templates/index.html"
    }
  ]
}
